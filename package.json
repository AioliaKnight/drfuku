{
  "name": "hemorrhoid-doctor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:velite": "velite --watch",
    "build": "velite && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "prepare": "husky install",
    "deploy": "cross-env NODE_ENV=production npm run build && node deploy.mjs",
    "deploy:staging": "cross-env NODE_ENV=staging npm run build && node deploy.mjs",
    "deploy:prod": "cross-env NODE_ENV=production npm run build && node deploy.mjs",
    "postbuild": "next-sitemap"
  },
  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all",
    "last 2 versions",
    "not ie <= 11"
  ],
  "dependencies": {
